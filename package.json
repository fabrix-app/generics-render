{
  "name": "@fabrix/generics-render",
  "version": "1.1.1",
  "description": "Generic: Render markdown with YAML as HTML and Metadata",
  "homepage": "https://fabrix.app",
  "author": {
    "name": "scott-wyatt",
    "email": "scottwyatt86@gmail.com",
    "url": "https://cali-style.com"
  },
  "keywords": [
    "engine",
    "cart",
    "generics",
    "render",
    "html",
    "markdown-it",
    "spool",
    "fabrix",
    "trailjs"
  ],
  "scripts": {
    "build": "tsc -p ./lib/tsconfig.release.json",
    "lint": "tslint -p ./lib",
    "watch": "tsc -w -p ./lib/tsconfig.release.json",
    "test": "npm run clean && npm run lint && npm run build && nyc mocha",
    "prepublishOnly": "npm run compile",
    "compile": "npm run clean && npm run build",
    "clean": "rm -rf dist",
    "ci": "cd .. && ci"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "pre-commit": [
    "test"
  ],
  "dependencies": {
    "markdown-it": "^8.3.0",
    "markdown-it-meta": "0.0.1"
  },
  "devDependencies": {
    "@fabrix/fabrix": "^1.1.3",
    "@fabrix/lint": "^1.0.0-alpha.3",
    "@fabrix/spool-generics": "^1.1.1",
    "@fabrix/spool-router": "^1.1.3",
    "@types/node": "~10.3.4",
    "lodash": "^4.17.10",
    "mocha": "^5",
    "nyc": "^12.0.2",
    "smokesignals": "^2.0.0",
    "supertest": "^2.0.1",
    "tslib": "~1.9.0",
    "tslint": "~5.10.0",
    "tslint-microsoft-contrib": "~5.0.3",
    "tsutils": "~2.27.1",
    "typescript": "~2.8.1"
  },
  "peerDependencies": {
    "@fabrix/spool-generics": "^1.1.1"
  },
  "engines": {
    "node": ">= 7.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:fabrix-app/generics-render.git"
  },
  "bugs": {
    "url": "https://github.com/fabrix-app/generics-render/issues"
  }
}
